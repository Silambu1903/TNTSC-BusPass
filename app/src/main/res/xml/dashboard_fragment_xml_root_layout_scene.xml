<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
        <KeyFrameSet></KeyFrameSet>
        <OnSwipe
            motion:dragDirection="dragLeft"
            motion:touchAnchorId="@+id/main_frame" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/viewDash"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/dashboard_icon"
            android:layout_width="40dp"
            android:layout_height="40dp"
            motion:layout_constraintBottom_toBottomOf="@id/viewDash"
            motion:layout_constraintEnd_toEndOf="@id/viewDash"
            motion:layout_constraintTop_toTopOf="@id/viewDash"
            motion:layout_constraintVertical_bias="0.05" />
        <Constraint
            android:id="@+id/main_frame"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/textView2"
            motion:layout_constraintEnd_toEndOf="@+id/pass_entry_view" />
        <Constraint
            android:id="@+id/view6"
            android:layout_width="0dp"
            android:layout_height="10dp"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            motion:layout_constraintEnd_toEndOf="@+id/txt_main"
            motion:layout_constraintStart_toStartOf="@id/main_frame"
            motion:layout_constraintTop_toBottomOf="@+id/txt_main" />
        <Constraint
            android:id="@+id/ic_siwpe_icon"
            android:layout_width="15dp"
            android:layout_height="15dp"
            motion:layout_constraintBottom_toBottomOf="@id/ic_swipe"
            motion:layout_constraintEnd_toEndOf="@id/ic_swipe"
            motion:layout_constraintStart_toStartOf="@id/ic_swipe"
            motion:layout_constraintTop_toTopOf="@id/ic_swipe"
            motion:layout_constraintVertical_bias="0.85" />
        <Constraint
            android:id="@+id/lottie_pass_entry"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:visibility="visible"
            motion:layout_constraintStart_toStartOf="@id/pass_entry_txt"
            motion:layout_constraintTop_toBottomOf="@id/ic_entry_line" />


        <Constraint
            android:id="@+id/sample_pass_entry_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
           android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginEnd="15dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="@id/lottie_pass_entry"
            motion:layout_constraintEnd_toEndOf="@id/pass_entry_view"
            motion:layout_constraintStart_toEndOf="@id/lottie_pass_entry"
            motion:layout_constraintTop_toTopOf="@id/lottie_pass_entry" />

        <Constraint
            android:id="@+id/lottie_pass_entry_1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:visibility="gone"
            motion:layout_constraintEnd_toEndOf="@id/pass_entry_view"
            motion:layout_constraintTop_toBottomOf="@id/rigth_arrow" />

        <Constraint
            android:id="@+id/sample_pass_entry_txt_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:visibility="gone"
            motion:layout_constraintBottom_toBottomOf="@id/lottie_pass_entry"
            motion:layout_constraintEnd_toStartOf="@id/lottie_pass_entry_1"
            motion:layout_constraintStart_toStartOf="@id/pass_entry_txt"
            motion:layout_constraintTop_toTopOf="@id/lottie_pass_entry" />
        <Constraint
            android:id="@+id/lottie_list_entry"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:visibility="gone"
            motion:layout_constraintStart_toStartOf="@id/pass_list_view"
            motion:layout_constraintTop_toBottomOf="@id/ic_list_line" />
        <Constraint
            android:id="@+id/sample_pass_list_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:visibility="gone"
            motion:layout_constraintBottom_toBottomOf="@id/lottie_list_entry"
            motion:layout_constraintEnd_toEndOf="@id/pass_list_view"
            motion:layout_constraintStart_toEndOf="@id/lottie_list_entry"
            motion:layout_constraintTop_toTopOf="@id/lottie_list_entry" />
        <Constraint
            android:id="@+id/lottie_pass_list_1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:layout_marginEnd="10dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toEndOf="@id/pass_list_view"
            motion:layout_constraintTop_toBottomOf="@id/ic_list_line" />
        <Constraint
            android:id="@+id/sample_pass_list_txt_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="@id/lottie_pass_list_1"
            motion:layout_constraintEnd_toStartOf="@id/lottie_pass_list_1"
            motion:layout_constraintStart_toStartOf="@id/pass_list_view"
            motion:layout_constraintTop_toTopOf="@id/lottie_pass_list_1" />
        <Constraint
            android:id="@+id/lottie_month_entry"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:visibility="visible"
            android:layout_marginStart="10dp"
            motion:layout_constraintTop_toBottomOf="@id/ic_month_line"
            android:layout_marginLeft="20dp"
            motion:layout_constraintStart_toStartOf="@id/pass_monthly_view"
            android:layout_marginTop="10dp" />
        <Constraint
            android:id="@+id/sample_pass_month_txt"
            motion:layout_constraintEnd_toEndOf="@id/pass_monthly_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="visible"
            motion:layout_constraintTop_toTopOf="@id/lottie_month_entry"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            motion:layout_constraintStart_toEndOf="@id/lottie_month_entry"
            motion:layout_constraintBottom_toBottomOf="@id/lottie_month_entry"
            android:layout_marginRight="10dp" />


