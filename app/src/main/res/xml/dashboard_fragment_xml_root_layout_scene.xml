            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:visibility="gone"
            motion:layout_constraintStart_toStartOf="@id/pass_entry_txt"
            motion:layout_constraintTop_toBottomOf="@id/ic_entry_line" />
        <Constraint
            android:id="@+id/lottie_pass_entry_1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toEndOf="@id/rigth_arrow"
            motion:layout_constraintTop_toBottomOf="@id/ic_entry_line" />

        <Constraint
            android:id="@+id/sample_pass_entry_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:visibility="gone"
            motion:layout_constraintBottom_toBottomOf="@id/lottie_pass_entry"
            motion:layout_constraintEnd_toEndOf="@id/pass_entry_view"
            motion:layout_constraintStart_toEndOf="@id/lottie_pass_entry"
            motion:layout_constraintTop_toTopOf="@id/lottie_pass_entry" />

        <Constraint
            android:id="@+id/sample_pass_entry_txt_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginStart="10dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="@id/lottie_pass_entry_1"
            motion:layout_constraintEnd_toStartOf="@id/lottie_pass_entry_1"
            motion:layout_constraintStart_toStartOf="@id/pass_entry_txt"
            motion:layout_constraintTop_toTopOf="@id/lottie_pass_entry_1" />
        <Constraint
            android:id="@+id/lottie_list_entry"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:visibility="visible"
            motion:layout_constraintStart_toStartOf="@id/pass_list_view"
            motion:layout_constraintTop_toBottomOf="@id/ic_list_line" />
        <Constraint
            android:id="@+id/sample_pass_list_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="@id/lottie_list_entry"
            motion:layout_constraintEnd_toEndOf="@id/pass_list_view"
            motion:layout_constraintStart_toEndOf="@id/lottie_list_entry"
            motion:layout_constraintTop_toTopOf="@id/lottie_list_entry" />
        <Constraint
            android:id="@+id/lottie_pass_list_1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:layout_marginEnd="20dp"
            android:visibility="gone"
            motion:layout_constraintEnd_toEndOf="@id/rigth_month_arrow"
            motion:layout_constraintTop_toBottomOf="@id/ic_list_line" />
        <Constraint
