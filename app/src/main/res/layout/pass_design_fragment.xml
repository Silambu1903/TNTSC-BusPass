<?xml version="1.0" encoding="utf-8"?>
<layout>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/passDetails_constraint"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <TextView
            android:id="@+id/textViewDatepass"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="18"
            android:textColor="@color/colorBlack"
            android:textSize="50sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.95"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.02" />

        <TextView
            android:id="@+id/txt_daypass"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="5dp"
            android:text="Mon"
            android:textColor="#808080"
            app:layout_constraintBottom_toBottomOf="@+id/textViewDatepass"
            app:layout_constraintEnd_toStartOf="@+id/textViewDatepass"
            app:layout_constraintTop_toTopOf="@+id/textViewDatepass"
            app:layout_constraintVertical_bias="0.3" />

        <TextView
            android:id="@+id/txt_month_year_pass"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="April 2020"
            android:textColor="#808080"
            app:layout_constraintBottom_toBottomOf="@+id/textViewDatepass"
            app:layout_constraintEnd_toEndOf="@+id/txt_daypass"
            app:layout_constraintTop_toTopOf="@+id/textViewDatepass"
            app:layout_constraintVertical_bias="0.8" />

        <TextView
            android:id="@+id/txt_main_pass"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PASS DETAILS"
            android:textColor="@color/colorBlack"
            android:textSize="35sp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.9"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txt_month_year_pass"
            app:layout_constraintVertical_bias="0.06" />

        <View
            android:id="@+id/view6_pass"
            android:layout_width="0dp"
            android:layout_height="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/ic_line"
            app:layout_constraintEnd_toEndOf="@+id/txt_main_pass"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txt_main_pass" />

        <View
            android:id="@+id/pass_entry_view"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="25dp"
            android:background="@drawable/blue_boder"
            android:visibility="visible"
            app:layout_constraintBottom_toTopOf="@id/pass_list_view"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view6_pass" />

        <View
            android:id="@+id/pass_list_view"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="25dp"
            android:background="@drawable/oragne_boder"
            android:visibility="visible"
            app:layout_constraintBottom_toTopOf="@id/pass_monthly_view"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/pass_entry_view" />

        <View
            android:id="@+id/pass_monthly_view"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_margin="25dp"
            android:background="@drawable/voliet_boder"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/pass_list_view" />

        <TextView
            android:id="@+id/pass_entry_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/passentry"
            android:textColor="@color/coloWhite"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@id/pass_entry_view"
            app:layout_constraintEnd_toEndOf="@id/pass_entry_view"
            app:layout_constraintHorizontal_bias="0.1"
            app:layout_constraintStart_toStartOf="@id/pass_entry_view"
            app:layout_constraintTop_toTopOf="@id/pass_entry_view"
            app:layout_constraintVertical_bias="0.15" />


        <View
            android:id="@+id/ic_entry_line"
            android:layout_width="0dp"
            android:layout_height="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/ic_line"
            app:layout_constraintEnd_toEndOf="@id/pass_entry_view"
            app:layout_constraintStart_toStartOf="@id/pass_entry_view"
            app:layout_constraintTop_toBottomOf="@id/pass_entry_txt" />

        <View
            android:id="@+id/lottie_pass_entry"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/ic_undraw_memory_storage_reh0"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@+id/pass_entry_view"
            app:layout_constraintStart_toStartOf="@+id/pass_entry_txt"
            app:layout_constraintTop_toBottomOf="@id/pass_entry_txt" />


        <TextView
            android:id="@+id/sample_pass_entry_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:text=" Manage Bus Pass Details Daily\nAnd View Old Pass"
            android:textColor="@color/coloWhite"
            android:textSize="10sp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@id/lottie_pass_entry"
            app:layout_constraintEnd_toEndOf="@+id/rigth_arrow"
            app:layout_constraintStart_toEndOf="@id/lottie_pass_entry"
            app:layout_constraintTop_toTopOf="@id/lottie_pass_entry" />


        <View
            android:id="@+id/rigth_arrow"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:background="@drawable/ic_right_arrow"
            app:layout_constraintBottom_toBottomOf="@id/pass_entry_txt"
            app:layout_constraintEnd_toEndOf="@id/pass_entry_view"
            app:layout_constraintHorizontal_bias="0.9"
            app:layout_constraintStart_toEndOf="@id/pass_entry_txt"
            app:layout_constraintTop_toTopOf="@id/pass_entry_txt" />


        <TextView
            android:id="@+id/pass_list_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="@string/passdailylist"
            android:textColor="@color/coloWhite"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@id/pass_list_view"
            app:layout_constraintEnd_toEndOf="@id/pass_list_view"
            app:layout_constraintHorizontal_bias="0.1"
            app:layout_constraintStart_toStartOf="@id/pass_list_view"
            app:layout_constraintTop_toTopOf="@id/pass_list_view"
            app:layout_constraintVertical_bias="0.15" />


        <View
            android:id="@+id/rigth_liest_arrow"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:background="@drawable/ic_right_arrow"
            app:layout_constraintBottom_toBottomOf="@id/pass_list_txt"
            app:layout_constraintEnd_toEndOf="@id/pass_list_view"
            app:layout_constraintHorizontal_bias="0.9"
            app:layout_constraintStart_toEndOf="@id/pass_list_txt"
            app:layout_constraintTop_toTopOf="@id/pass_list_txt" />


        <View
            android:id="@+id/lottie_list_entry"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/ic_undraw_date_picker_gorr"
            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="@id/pass_list_view"
            app:layout_constraintTop_toBottomOf="@id/ic_list_line" />


        <TextView
            android:id="@+id/sample_pass_list_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:text=" Manage Bus Pass Details Daily \n And View Old Pass"
            android:textColor="@color/coloWhite"
            android:textSize="10sp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@id/lottie_list_entry"
            app:layout_constraintEnd_toStartOf="@id/lottie_list_entry"
            app:layout_constraintStart_toStartOf="@+id/pass_list_txt"
            app:layout_constraintTop_toTopOf="@id/lottie_list_entry" />


        <View
            android:id="@+id/ic_list_line"
            android:layout_width="0dp"
            android:layout_height="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/ic_line"
            app:layout_constraintEnd_toEndOf="@id/pass_list_view"
            app:layout_constraintStart_toStartOf="@id/pass_list_view"
            app:layout_constraintTop_toBottomOf="@id/pass_list_txt" />

        <TextView
            android:id="@+id/pass_month_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="@string/monthly_pass_report"
            android:textColor="@color/coloWhite"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@id/pass_monthly_view"
            app:layout_constraintEnd_toEndOf="@id/pass_monthly_view"
            app:layout_constraintHorizontal_bias="0.1"
            app:layout_constraintStart_toStartOf="@id/pass_monthly_view"
            app:layout_constraintTop_toTopOf="@id/pass_monthly_view"
            app:layout_constraintVertical_bias="0.15" />


        <View
            android:id="@+id/rigth_month_arrow"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:background="@drawable/ic_right_arrow"
            app:layout_constraintBottom_toBottomOf="@id/pass_month_txt"
            app:layout_constraintEnd_toEndOf="@id/pass_monthly_view"
            app:layout_constraintHorizontal_bias="0.9"
            app:layout_constraintStart_toEndOf="@id/pass_month_txt"
            app:layout_constraintTop_toTopOf="@id/pass_month_txt" />

        <View
            android:id="@+id/ic_month_line"
            android:layout_width="0dp"
            android:layout_height="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/ic_line"
            app:layout_constraintEnd_toEndOf="@id/pass_monthly_view"
            app:layout_constraintStart_toStartOf="@id/pass_monthly_view"
            app:layout_constraintTop_toBottomOf="@id/pass_month_txt" />

        <View
            android:id="@+id/lottie_month_entry"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/ic_undraw_mobile_posts_tlwg"
            android:visibility="visible"
            app:layout_constraintStart_toStartOf="@+id/pass_month_txt"
            app:layout_constraintTop_toBottomOf="@id/ic_month_line" />


        <TextView
            android:id="@+id/sample_pass_month_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:text=" Manage Bus Pass Details Daily\nAnd View Old Pass "
            android:textColor="@color/coloWhite"
            android:textSize="10sp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@id/lottie_month_entry"
            app:layout_constraintEnd_toEndOf="@+id/rigth_month_arrow"
            app:layout_constraintStart_toEndOf="@id/lottie_month_entry"
            app:layout_constraintTop_toTopOf="@id/lottie_month_entry" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
</layout>